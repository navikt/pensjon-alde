{
  "name": "alde",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "dev": "react-router dev",
    "dev:mock": "NODE_ENV=mock react-router dev",
    "start": "react-router-serve ./build/server/index.js",
    "lint": "biome lint .",
    "lint:fix": "biome lint --write .",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "check": "biome check .",
    "check:fix": "biome check --write .",
    "ci": "biome ci .",
    "test": "vitest",
    "test:ci": "export CI=true && vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "typecheck": "react-router typegen && tsc",
    "prepare": "husky"
  },
  "dependencies": {
    "@grafana/faro-web-sdk": "^1.19.0",
    "@grafana/faro-web-tracing": "^1.19.0",
    "@navikt/aksel-icons": "^7.32.3",
    "@navikt/ds-css": "^7.32.3",
    "@navikt/ds-react": "^7.32.3",
    "@navikt/ds-tokens": "^7.31.0",
    "@react-router/node": "^7.9.3",
    "@react-router/serve": "^7.9.3",
    "clsx": "^2.1.1",
    "compression": "^1.8.1",
    "date-fns": "^4.1.0",
    "express": "^5.1.0",
    "isbot": "^5.1.31",
    "node-cache": "^5.1.2",
    "react": "^19.2.0",
    "react-auth": "^1.0.0",
    "react-dom": "^19.2.0",
    "react-router": "^7.9.3",
    "remix-auth-oauth2": "^3.4.1"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.6",
    "@playwright/test": "^1.55.1",
    "@react-router/dev": "^7.9.3",
    "@tailwindcss/vite": "^4.1.15",
    "@types/node": "^24.9.1",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "msw": "^2.11.3",
    "react-router-devtools": "^5.1.3",
    "typescript": "^5.9.3",
    "vite": "^7.1.9",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "lint-staged": {
    "!(public)/**/*.{js,jsx,ts,tsx,json,css,scss}": [
      "biome check --write --files-ignore-unknown=true"
    ]
  }
}
